# Camaral RAG Chatbot â€” environment template
# Copy to .env and fill in values.

# Database (PostgreSQL 16 with pgvector)
DATABASE_URL=postgresql+asyncpg://user:pass@localhost:5432/camaral

# Google AI (Gemini API)
GOOGLE_API_KEY=

# Telegram
TELEGRAM_BOT_TOKEN=

# Logfire (optional; if set, observability is enabled)
LOGFIRE_TOKEN=

# RAG
RAG_TOP_K=5
EMBEDDING_MODEL=text-embedding-004
RAG_SIMILARITY_THRESHOLD=0.85

# Chat & memory
CHAT_HISTORY_LIMIT=20
CONVERSATION_TIMEOUT_SECONDS=3600

# Rate limiting (per user)
RATE_LIMIT_REQUESTS=20
RATE_LIMIT_WINDOW_SECONDS=60
